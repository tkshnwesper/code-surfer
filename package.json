{
  "name": "prism-surfer",
  "private": false,
  "version": "0.0.1",
  "license": "MIT",
  "author": "tkshnwesper",
  "repository": "https://github.com/tkshnwesper/prism-surfer",
  "scripts": {
    "build:packages": "lerna run build --no-private",
    "build:watch": "lerna exec 'npm run build:watch --no-private --parallel'",
    "format": "prettier --write '**/*.{js,jsx,css,md}'",
    "format:check": "prettier-check '**/*.{js,jsx,css,md}'",
    "test": "run-p format:check test:all",
    "test:all": "lerna run test"
  },
  "devDependencies": {
    "lerna": "^3.2.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0"
  },
  "dependencies": {}
}
